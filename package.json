{
  "name": "my-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pnpm --filter @my-monorepo/app1 start",
    "start:app1": "pnpm --filter @my-monorepo/app1 start",
    "start:app2": "pnpm --filter @my-monorepo/app2 start",
    "build": "pnpm --filter @my-monorepo/shared build && pnpm --filter @my-monorepo/app1 build && pnpm --filter @my-monorepo/app2 build",
    "build:shared": "pnpm --filter @my-monorepo/shared build",
    "build:app1": "pnpm --filter @my-monorepo/app1 build",
    "build:app2": "pnpm --filter @my-monorepo/app2 build",
    "test": "pnpm --filter @my-monorepo/app1 test && pnpm --filter @my-monorepo/app2 test",
    "create-app": "node scripts/create-app.js",
    "install:all": "pnpm install"
  },
